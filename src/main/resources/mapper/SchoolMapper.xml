<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.flong.springboot.mediatorPattern.matter.ISchoolDao">


    <select id="querySchoolInfoById" resultType="com.flong.springboot.mediatorPattern.matter.School">
        SELECT id, name, address, createTime, updateTime
        FROM school
        where id = #{id}
    </select>

</mapper>